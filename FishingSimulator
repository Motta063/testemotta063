local AutoFishToggle = Section1:CreateToggle("AutoFish", nil, function(State)
    go = State
    while go do
        task.wait(2.6)
        game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.FishCaught:FireServer()
        game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.processGameItemSold:InvokeServer("SellEverything")
    end
end)

local WalkOnWaterToggle = Section1:CreateToggle("WalkOnWater", nil, function(State)
    t = State
    game:GetService("Workspace").Water.CanCollide = t
end)

-- Exemplo de correção no connect e verificação de HumanoidRootPart
local AutoKillMobsToggle = Section1:CreateToggle("Auto Kill Mobs", nil, function(State)
    getgenv().Roof = State
    game:GetService("RunService").Stepped:Connect(function()
        if getgenv().Roof then
            local character = game.Players.LocalPlayer.Character
            if character and character:FindFirstChild("Humanoid") then
                character.Humanoid:ChangeState(11)
            end
        end
    end)

    while task.wait(1) and getgenv().Roof do
        pcall(function()
            local tool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
            if tool and tool:FindFirstChild("GripC1") and getNearestMob() then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getNearestMob().HumanoidRootPart.CFrame
                for i = 1, 100 do
                    game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.MonsterHit:FireServer(getNearestMob(), tool.Name, true)
                end
                game:GetService("ReplicatedStorage").CloudFrameShared.DataStreams.processGameItemSold:InvokeServer("SellEverything")
            end
        end)
    end
end)
